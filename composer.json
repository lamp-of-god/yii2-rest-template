{
    "name": "lamp-of-god/yii2-rest",
    "description": "Rest API server template based on Yii2",
    "type": "project",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.3.0",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "yii2mod/yii2-swagger": "*"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
